Algoritmo mostrardigitos
	Definir num, digito1, digito2 Como Entero;
	
    Escribir "Ingrese un número entero de dos dígitos:";
    Leer num;
    digito1 <- num / 10;
    digito2 <- num% 10;
    Escribir "El primer dígito es:", digito1;
    Escribir "El segundo dígito es:", digito2;

	
FinAlgoritmo
